<h1> search result </h1>

<% @listings.each do |list| %>




<p>
	
	Property Name:<%= list.property_name %> <br />
	Room Type:<%= list.room_type %> <br />
	Total Guest:<%= list.total_guest %> <br />
	Country:<%= list.country %> <br /> 
	<%= link_to 'Edit', edit_listing_path(list)%> <br />
	<%= link_to 'Delete', list, method: :delete %>
	<%= link_to 'Show', listing_path(list)%> <br />
	</p>
	
<% end %>